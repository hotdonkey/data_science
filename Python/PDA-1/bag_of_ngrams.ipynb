   ],
   "source": [
    "from nltk.util import ngrams\n",
    "text = \"I like this movie, it's funny. I hate this movie. This was awesome! I like it. Nice one. I love it.\"\n",
    "tokenized = text.split()\n",
    "bigrams = ngrams(tokenized, 2)\n",
    "\n",
    "[i for i in bigrams]"
   ]
